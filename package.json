{
  "name": "mail",
  "version": "1.0.0",
  "description": "Angular2.0 Sample in Office Mail",
  "main": "index.js",
  "scripts": {
    "start": "concurrent \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "postinstall": "typings install",
    "test": "live-server --open=unit-test.html"
